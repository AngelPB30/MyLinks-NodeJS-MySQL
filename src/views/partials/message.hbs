{{#if success }}
<div class="row mb-3">
	<div class="col-md-4 mx-auto">
		<div class="alert alert-success alert-dismissible fade show m-0 messageAlert" role="alert">
			<i class="far fa-laugh-wink"></i> {{success}}
			<button type="button" class="close" data-dismiss="alert" aria-label="close">
				<span aria-hidden="true">&times;</span>
			</button>
		</div>
	</div>
</div>
{{/if}}

{{#if message }}
<div class="row mb-3">
	<div class="col-md-4 mx-auto">
		<div class="alert alert-danger alert-dismissible fade show m-0 messageAlert" role="alert">
			{{message}}
			<button type="button" class="close" data-dismiss="alert" aria-label="close">
				<span aria-hidden="true">&times;</span>
			</button>
		</div>
	</div>
</div>
{{/if}}